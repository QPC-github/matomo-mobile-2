"NavigationWindow": {
    modal: true
}

".iOSContainer": {
    backgroundColor: "white",
    title: L('General_ChooseDate')
}

"#datePickerTable": {
    top: 0,
    zIndex: 4
}

"#fromPicker": {
    bottom: 0,
    zIndex: 10,
    height: Ti.UI.SIZE,
    width: Ti.UI.FILL,
    left: 0,
    type: Ti.UI.PICKER_TYPE_DATE,
    datePickerStyle: Ti.UI.iOS.DATE_PICKER_STYLE_WHEELS,
}

"#toPicker": {
    bottom: 0,
    left: 0,
    height: Ti.UI.SIZE,
    width: Ti.UI.FILL,
    zIndex: 8,
    type: Ti.UI.PICKER_TYPE_DATE,
    datePickerStyle: Ti.UI.iOS.DATE_PICKER_STYLE_WHEELS,
    visible: false
}

"#iOSPeriodPicker": {
    bottom: 0,
    left: 0,
    height: Ti.UI.SIZE,
    width: Ti.UI.FILL,
    zIndex: 6,
    visible: false,
    selectionIndicator: true
}

"#datePickerTable[platform=ios]": {
    style: Titanium.UI.iOS.TableViewStyle.GROUPED,
}

"#doneButton": {
    title: L('General_Done')
}

"#doneButton[platform=ios]": {
    style: Ti.UI.iOS.SystemButtonStyle.DONE,
    tintColor: '#ffffff'
}

"#cancelButton": {
    title: L('General_Cancel')
}

"#cancelButton[platform=ios]": {
    style: Ti.UI.iOS.SystemButtonStyle.CANCEL,
    tintColor: '#ffffff'
}